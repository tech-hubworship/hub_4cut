# 허브 인생네컷 프로젝트 환경 변수 설정

# API 설정
API_BASE_URL=https://api.hub-photobooth.com
API_TIMEOUT=30000

# 프린터 설정
PRINTER_IP=192.168.1.100
PRINTER_PORT=9100

# 클라우드 설정
CLOUD_BUCKET=hub-photobooth
CLOUD_REGION=ap-northeast-2

# 개발 환경 설정
NODE_ENV=development
DEBUG=true

# 로깅 설정
LOG_LEVEL=info
LOG_FILE_PATH=./logs/app.log

# 보안 설정
JWT_SECRET=your-jwt-secret-key
ENCRYPTION_KEY=your-encryption-key

# 데이터베이스 설정 (백엔드용)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=hub_photobooth
DB_USER=postgres
DB_PASSWORD=your-db-password

# Redis 설정 (백엔드용)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password

# 이메일 설정 (백엔드용)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# SMS 설정 (백엔드용)
SMS_API_KEY=your-sms-api-key
SMS_SENDER_ID=HUB_PHOTO

# 결제 설정 (백엔드용)
PAYMENT_API_KEY=your-payment-api-key
PAYMENT_SECRET=your-payment-secret

# 소셜 로그인 설정 (백엔드용)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
FACEBOOK_APP_ID=your-facebook-app-id
FACEBOOK_APP_SECRET=your-facebook-app-secret

# 파일 업로드 설정
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=jpg,jpeg,png,gif,webp
UPLOAD_PATH=./uploads

# 캐시 설정
CACHE_TTL=3600
CACHE_MAX_SIZE=100

# 알림 설정
PUSH_NOTIFICATION_KEY=your-push-notification-key
PUSH_NOTIFICATION_SECRET=your-push-notification-secret

# 모니터링 설정
SENTRY_DSN=your-sentry-dsn
NEW_RELIC_LICENSE_KEY=your-new-relic-license-key

# 테스트 설정
TEST_DB_NAME=hub_photobooth_test
TEST_API_BASE_URL=http://localhost:3001
